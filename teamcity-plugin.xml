<teamcity-plugin xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>sonar-plugin</name>
    <display-name>Sonar runner</display-name>
    <version>@Version@</version>
    <description>Allows configuring and running SonarQube analysis on the TeamCity server</description>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
