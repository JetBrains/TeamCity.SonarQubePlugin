<?xml version="1.0" encoding="UTF-8"?>

<teamcity-plugin xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>sonar-plugin</name>
    <display-name>Sonar runner</display-name>
    <version>${sonar.plugin.version}</version>
    <description>${project.description}</description>
    <vendor>
      <name>${sonar.plugin.vendor}</name>
      <url>${sonar.plugin.vendorUrl}</url>
    </vendor>
  </info>
  <requirements min-build="78475" />
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>